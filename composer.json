    "type": "psalm-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Evgeny Savich",
            "email": "jack.savich@gmail.com"
        },
        {
            "name": "David Feller",
            "email": "david.feller@normvarianz.de"
        }
    ],
    "require": {
